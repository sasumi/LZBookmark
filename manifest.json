{
	"manifest_version": 2,
	"name": "LzBookmark",
	"version": "1.0",
	"description": "Lazy bookmark manager plugin",
	"author": "sasumi",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"homepage_url": "http://blog.yikepi.com",
	"permissions": [
		"contextMenus",
		"bookmarks",
		"chrome://favicon/",
		"http://*/",
		"https://*/"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
	]
}