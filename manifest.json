{
	"manifest_version": 2,
	"name": "LzBookmark",
	"version": "1.0",
	"description": "Lazy bookmark manager plugin",
	"author": "sasumi",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		"bookmarks"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.baidu.com/*"
			],
			"js": [
				"assert/js/jquery-1.8.3.min.js",
				"assert/js/content-script.js"
			],
			"run_at": "document_end"
		}
	],
	"background": {
		"scripts": [
			"assert/js/background.js"
		]
	},
	"web_accessible_resources": [
	]
}